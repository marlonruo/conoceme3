<?xml version='1.0' encoding='utf-8'?>
<widget id="com.conoceme_mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="ApplozicCordovaPlugin">
        <param name="android-package" value="com.applozic.phonegap.ApplozicCordovaPlugin" />
    </feature>
    <name>ConoceME!</name>
    <description>
        Con ConóceME! vas a conocer gente para hacer nuevos amigos de América Latina.
    </description>
    <author email="marlonruo@gmail.com" href="conoceme.vip">
        Nospun
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
