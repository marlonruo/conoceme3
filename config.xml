<?xml version='1.0' encoding='utf-8'?>
<widget id="com.conoceme_mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ConoceME!</name>
    <description>
        Con ConóceME! vas a conocer gente para hacer nuevos amigos de América Latina.
    </description>
    <author email="marlonruo@gmail.com" href="conoceme.vip">
        Nospun
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-statusbar" spec="^2.3.0" />
    <plugin name="cordova-opentok-android-permissions" spec="^1.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.1.0" />
    <plugin name="cordova-plugin-media-capture" />
    <plugin name="cordova-plugin-inappbrowser" />
    <plugin name="cordova-plugin-media" />
    <plugin name="cordova-plugin-vibration" />
    <plugin name="com.applozic.phonegap" spec="https://github.com/AppLozic/Applozic-Cordova-Ionic-PhoneGap-Chat-Plugin.git" />
</widget>
